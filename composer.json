{
  "name": "vinteenove/vimu-app-installer-plugin",
  "description": "Instala aplicativos no VIMU",
  "type": "composer-plugin",
  "require": {
    "php": ">=7.4 || >=8.0",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": ">=2"
  },
  "autoload": {
    "psr-4": {
      "\\Vimu\\Composer\\AppInstallerPlugin\\": "./src"
    }
  },
  "extra": {
    "class": "Vimu\\Composer\\AppInstallerPlugin\\AppInstallerPlugin"
  }
}